# Основной дисплей
load-display pandagl

# Резервные варианты
aux-display p3dx11
aux-display p3dwin
aux-display p3dtiny

# Пути к плагинам и ресурсам
model-path $MAIN_DIR
plugin-path $MAIN_DIR/plugins
texture-path $MAIN_DIR/assets
texture-path $MAIN_DIR/textures

# Настройки окна
win-size 1280 720
window-title StepDefence
fullscreen false